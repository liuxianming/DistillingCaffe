layer {
  name: "feat_p"
  type: "Pooling"
  bottom: "conv6_p"
  top: "feat_p"
  pooling_param {
    pool: AVE
    global_pooling: true
  }
}
layer {
  name: "l2norm_p"
  type: "Normalize"
  bottom: "feat_p"
  top: "feat_norm_p"
}
layer {
  name: "loss"
  type: "ContrastiveLoss"
  bottom: "feat_norm"
  bottom: "feat_norm_p"
  bottom: "sim"
  top: "loss"
  contrastive_loss_param {
    margin: 1
  }
}
